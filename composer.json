{
    "name": "mchuluq/laravel-mfa",
    "description": "Multi-Factor Authentication package for Laravel with TOTP, Email OTP, and WebAuthn support",
    "keywords": [
        "laravel",
        "mfa",
        "2fa",
        "two-factor",
        "authentication",
        "totp",
        "otp",
        "webauthn",
        "security"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mochammad Chuluq",
            "email": "mchuluq@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/auth": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/cookie": "^8.0",
        "illuminate/encryption": "^8.0",
        "illuminate/mail": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/session": "^8.0",
        "illuminate/validation": "^8.0",
        "spomky-labs/otphp": "^10.0",
        "bacon/bacon-qr-code": "^2.0",
        "web-auth/webauthn-lib": "^3.3",
        "web-auth/cose-lib": "^3.3",
        "ramsey/uuid": "^4.0",
        "paragonie/constant_time_encoding": "^2.4"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "mockery/mockery": "^1.4",
        "phpstan/phpstan": "^0.12",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Mchuluq\\LaravelMFA\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mchuluq\\LaravelMFA\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mchuluq\\LaravelMFA\\MFAServiceProvider"
            ],
            "aliases": {
                "MFA": "Mchuluq\\LaravelMFA\\Facades\\MFA"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix",
        "analyse": "vendor/bin/phpstan analyse"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}